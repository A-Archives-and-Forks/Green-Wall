{
  "name": "green-wall",
  "private": true,
  "description": "GitHub contributions graph. Review all the contributions you've made to GitHub over the years.",
  "license": "UNLECENSED",
  "scripts": {
    "build": "next build",
    "deps": "yarn upgrade-interactive --latest",
    "dev": "next dev",
    "lint": "next lint && npm run lint:style && npm run lint:ts",
    "lint:es": "eslint \"**/*.{mjs,js,jsx,ts,tsx}\"",
    "lint:style": "stylelint \"**/*.{css}\"",
    "lint:ts": "tsc --noEmit --skipLibCheck",
    "lint:prettier": "prettier --write \"**/*.{md,json,html}\"",
    "start": "next start"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.12",
    "classnames": "^2.3.1",
    "html2canvas": "^1.4.1",
    "next": "12.2.5",
    "nextjs-cors": "^2.1.1",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@tailwindcss/line-clamp": "^0.4.0",
    "@types/node": "18.7.13",
    "@types/react": "18.0.17",
    "autoprefixer": "^10.4.8",
    "eslint-plugin-tailwindcss": "^3.6.0",
    "postcss": "^8.4.16",
    "prefer-code-style": "0.6.2",
    "tailwindcss": "^3.1.8",
    "typescript": "^4.8.2"
  }
}
